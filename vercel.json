{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*", "public/**/*", "tsconfig.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
