{
  "name": "all-in-one-tools-bun",
  "version": "1.0.0",
  "description": "All-in-one tools with Bun.js and TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "node dist/index.js",
    "build": "bun build src/index.ts --outdir dist --target node --format cjs",
    "type-check": "bun tsc --noEmit"
  },
  "keywords": ["bun", "typescript", "express", "tools"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.2",
    "marked": "^15.0.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
